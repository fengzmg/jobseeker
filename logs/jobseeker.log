   	function createJobAlert(elem) {
   		var email = elem.find("input[name='email']").val();
   		if(!isValidEmail(email)) {
   			elem.find(".error_msg").text("Not a valid email");
   			elem.find(".error_msg").show();
   			return false;
   		}
   		elem.find(".saving-msg").show();
   		$.ajax({
   			type: "POST",
   			url: elem.attr("action"),
   			dataType: "json",
   			data: elem.serialize(),
   			success: function(response) {
   				elem.find(".saving-msg").hide();	    				
   				if(response.type=='error') {
   					elem.find(".error_msg").text(response.message);
   					elem.find(".error_msg").show();				
   				} else {
   					elem.find(".error_msg").hide();
   					$(".main-alert-area").html("<div style='padding: 0 5px;'>" + response.message + "</div>");
   					$.colorbox.resize();
   					var q = elem.find("input[name='query']").val();
   					var l = elem.find("input[name='location']").val();
   					var r = elem.find("input[name='region']").val();
   					
   					var alertPos = "Results - Bottom";
   					if(elem.find(".main-alert-area").attr("class").toLowerCase().indexOf("sidebar") >= 0)
   			   			alertPos = "Results - Sidebar";
   					else if(elem.attr("id") == 'alert-popup-confirm-form') 
   						alertPos = "Results - New popup";
   			   		
   					_gaq.push(['_trackEvent', 'Job Search Results', 'Job Alert Created', q + " - " + l + " - " + r,, true]);
   					_gaq.push(['_trackEvent', 'Job alert created', alertPos, q + " - " + l + " - " + r,,'true']);
   					
   					
   					
   				}		
   				
   				if(elem.attr("id") == 'alert-popup-confirm-form') {
   		   		// Hide the window after 4 seconds
   			   		window.setTimeout(function() {
   			   		elem.find(".main-alert-area").fadeOut("slow");
   			        }, 3000);
   		   			
   		   		}
   			},
   			error: function(response) {
   				$(".saving-msg").hide();
   				$(".error").text("Due to some technical difficulties we cannot process your request now. Please try again later.");
   				$(".error").show();			
   			}
   		});
   	}
   	
