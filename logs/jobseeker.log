	}
	
	function showFlyout() {
		if(!$("#flyout").is(":visible")) {
			$("#flyout").removeClass("hide-from-all");
			$("#flyout").addClass("hidden-desktop");			
		}
		$("#content" ).animate({"left": "+=250px" }, 100, "linear",function() {
			isFlyoutVisible = true;
			$("#content").css("position","fixed"); 
		});		
	}
	
	function hideFlyout() {
		if(isFlyoutVisible) {
			$("#content" ).animate({"left": "-=250px" }, 100, "linear",function() {
				isFlyoutVisible = false
				$("#content").css("position","absolute"); 
			});		
		}
	}
	
